[user]
	email = adrian.shaw@arm.com
	name = Adrian L. Shaw
[color]
	ui = auto
	status = auto
	branch = auto

[alias]
	co = checkout
	all = "!f() { ls | xargs -I{} git -C {} $1; }; f"
	tree = log --graph --pretty=format:'%C(auto) %h %ad [%an] %m%m %d %s' --abbrev-commit --all --decorate --date=short
	b = branch
	br = branch
	dif = diff --stat
	s = status
	ad = add
	p = push
	puhs = push
	pul = pull
	undo = reset --soft HEAD~1
